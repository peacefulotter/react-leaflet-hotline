{
  "name": "react-leaflet-hotline",
  "version": "1.0.3",
  "description": "Ported Leaflet.Hotline to React and Typescript. Create leaflet Polylines with colored gradients",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo 'Error, not yet implemented'",
    "clean": "rm -rf ./lib",
    "prebuild": "npm run clean",
    "build-cjs": "tsc --project ./tsconfig.cjs.json",
    "build-esm": "tsc --project ./tsconfig.esm.json",
    "build-types": "tsc --project ./tsconfig.types.json",
    "build": "npm run build-cjs && npm run build-esm && npm run build-types && echo '\\033[1;32m Successfully built \\033[0m'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "build-docs": "npx del-cli dist-docs && npx styleguidist build",
    "serve-docs": "npx styleguidist server"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeacefulOtter/react-leaflet-hotline.git"
  },
  "keywords": [
    "leaflet",
    "gradient",
    "react",
    "colors",
    "color",
    "hotline",
    "map"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": {
    "name": "PeacefulOtter",
    "email": "nathan.maire@epfl.ch",
    "url": "https://natmai.netlify.app"
  },
  "license": "MIT",
  "dependencies": {
    "leaflet": "^1.8.0",
    "react": "^18.2.0",
    "react-leaflet": "^4.0.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@types/leaflet": "^1.7.11",
    "@types/react": "^18.0.14",
    "@types/react-leaflet": "^2.8.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "glob": "^8.0.3",
    "react-styleguidist": "^11.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0"
  }
}
